{
  "name": "benjamin.parry.is-indiekit",
  "version": "1.0.0",
  "description": "Indiekit server for https://benjamin.parry.is",
  "main": "index.js",
  "scripts": {
    "start": "indiekit serve"
  },
  "keywords": [
    "indiekit",
    "indieweb"
  ],
  "author": "",
  "license": "ISC",
  "engines": {
    "node": ">=20"
  },
  "type": "module",
  "indiekit": {
    "plugins": [
      "@indiekit/store-github"
    ],
    "publication": {
      "me": "https://benjamin.parry.is"
    },
    "@indiekit/store-github": {
      "user": "benjaminparry",
      "repo": "benjamin.parry.is-indiekit",
      "branch": "main"
    }
  },
  "dependencies": {
    "@indiekit/indiekit": "^1.0.0-beta.19",
    "@indiekit/store-github": "^1.0.0-beta.19"
  }
}
